#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    int arr[]={1,34,5,6};
    int  i,j ,elem,found=0;
    elem=arr[0];
    for(int i=0;i<4;i++)
    {if(arr[i]==elem)
    {for(j=i;j<3;j++)
    arr[j]=arr[j+1];
    found++;
    i--;
    }
    }
        cout<<"new array is"<<endl;
    for ( i = 0; i <3; i++)
    {
            cout<<arr[i]<<endl;
    }
}
