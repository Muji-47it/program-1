#include <iostream>
using namespace std;
class middle{
private:
int arr[10]={99,48,34,57,65,23,54,22,43,21};
int num=0;
public:
/*void input()
{
    cout<<"enter values: "<<endl;
    for(int i=0;i<10;i++)
    cin>>arr[i];
}*/
void add()
{for(int i=0;i<10;i++)
 num+=arr[i];
}
void output()
{
    cout<<"sum of array values"<<endl;
    cout<<num;
    int n=num/10;
    cout<<"average value is"<<endl;
    cout<<n;
    int min=n;
    cout<<"minimum values of array";
    for(int i=0;i<10;i++)
    {if(min>arr[i])
    min=arr[i];
    cout<<min<<endl;}
}
};
int main() {
    middle m;
  //  m.input();
    m.add();
    m.output();
}
