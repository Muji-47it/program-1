#include <iostream>
using namespace std;
class array{
private:
int arr[10]={99,48,34,57,65,23,54,22,43,21};
int num=0;
public:
void add()
{for(int i=0;i<10;i++)
 num+=arr[i];
}
void output()
{
    cout<<"sum of array values"<<endl;
    cout<<num<<endl;
    double avg=num/10;
    cout<<"average value is"<<endl;
    cout<<avg;
    
}
};
int main() {
    array m;
    m.add();
    m.output();
}
