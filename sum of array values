#include <iostream>
using namespace std;
class middle{
private:
int arr[100];
int num=0;
public:
void input()
{
    cout<<"enter values: "<<endl;
    for(int i=0;i<6;i++)
    cin>>arr[i];
}
void add()
{for(int i=0;i<6;i++)
 num+=arr[i];
}
void output()
{
    cout<<"sum of array values"<<endl;
    cout<<num;
}
};
int main() {
    middle m;
    m.input();
    m.add();
    m.output();
}
